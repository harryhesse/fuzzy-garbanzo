<h1>All Users</h1>
<a href="/users/new">Create New User</a>
<ul>
  <% users.forEach(user => { %>
  <li>
    <a href="/users/<%= user._id %>"><%= user.name %></a>
    (<a href="/users/<%= user._id %>/edit">Edit</a>)
    <form
      action="/users/<%= user._id %>/delete"
      method="POST"
      style="display: inline"
    >
      <button>Delete</button>
    </form>
  </li>
  <% }) %>
</ul>
